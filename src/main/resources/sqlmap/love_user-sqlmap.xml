                    </isNotNull>
				                                                <isNotNull prepend="," property="houseStatus">
                        house_status = #houseStatus#
                    </isNotNull>
				                                                <isNotNull prepend="," property="carStatus">
                        car_status = #carStatus#
                    </isNotNull>
				                                                <isNotNull prepend="," property="status">
                        status = #status#
                    </isNotNull>
				                                                        					<dynamic prepend=",">
						modify_time=UNIX_TIMESTAMP()
