            <iterate conjunction="," open="(" close=")" property="keys">
                #keys[]#
            </iterate>
        </select>
                <delete id="LoveUser.deleteByKey">
            DELETE FROM love_user
            WHERE id = #key#
        </delete>
                <delete id="LoveUser.deleteByKeys">
            DELETE FROM love_user
            WHERE id in
            <iterate conjunction="," open="(" close=")" property="keys">
