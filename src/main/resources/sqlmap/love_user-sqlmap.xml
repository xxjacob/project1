    <!-- Select SQL -->
    <sql id="loveUser.selector">
        SELECT id,openid,nickname,gender,birth_y,birth_md,figureurl,region,introduce,love_status,height,education,blood_group,earning,profession,house_status,car_status,status,create_time,modify_time,invitor            FROM love_user
    </sql>
    <sql id="loveUserList.where">
        <dynamic>
                                                <isNotNull property="id" prepend=" AND ">
                        id=#id#
                    </isNotNull>
                                                                <isNotNull property="openid" prepend=" AND ">
                        openid=#openid#
                    </isNotNull>
                                                                <isNotNull property="nickname" prepend=" AND ">
                        nickname=#nickname#
                    </isNotNull>
                                                                                            <isNotNull property="birthY" prepend=" AND ">
                        birth_y=#birthY#
                    </isNotNull>
                                                                <isNotNull property="birthMd" prepend=" AND ">
                        birth_md=#birthMd#
                    </isNotNull>
                                                                <isNotNull property="figureurl" prepend=" AND ">
                        figureurl=#figureurl#
                    </isNotNull>
                                                                <isNotNull property="region" prepend=" AND ">
                        region=#region#
                    </isNotNull>
                                                                <isNotNull property="introduce" prepend=" AND ">
                        introduce=#introduce#
                    </isNotNull>
                                                                                            <isNotNull property="height" prepend=" AND ">
                        height=#height#
                    </isNotNull>
                                                                                                                                                                                                                                                                    <isNotNull property="createTime" prepend=" AND ">
                        create_time=#createTime#
                    </isNotNull>
                                                                <isNotNull property="modifyTime" prepend=" AND ">
                        modify_time=#modifyTime#
                    </isNotNull>
                                                                <isNotNull property="invitor" prepend=" AND ">
                        invitor=#invitor#
                    </isNotNull>
                                    </dynamic>
    </sql>
        <sql id="loveUserList.order">
        <dynamic prepend=" ORDER BY ">
            <isNotEmpty property="orderFields">
                <iterate property="orderFields" conjunction="," open="" close="">
                    #orderFields[].fieldName# #orderFields[].order#
                </iterate>
            </isNotEmpty>
        </dynamic>
    </sql>
            <select id="LoveUser.getLoveUser" resultMap="loveUser">
            <include refid="loveUser.selector" />
            WHERE id=#id#
        </select>
                <select id="LoveUser.getLoveUsersByKeys" resultMap="loveUser">
            <include refid="loveUser.selector" />
